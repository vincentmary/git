[alias]

# log aliases
l='log --pretty=format:"%C(yellow)%h\\ %ad%Cred%d\\ %Creset%s%Cblue\\ [%cn]" --decorate --date=short'
ls="hist | grep"
 
# checkout aliases
co="checkout"
cob="checkout -b"
col="checkout @{-1}"
com="checkout master"
 
#stash aliases
st="stash"
stl="stash list"
 
# commit aliases
amend = commit --amend -m


# list aliases
la="!git config -l | grep alias | cut -c 7-"
 
# Diff aliases
d="diff"
ds = diff --staged

# status aliases
st="status"

# branch aliases
merged="branch --merge"

#reset aliases
unstage="reset --soft HEAD^"
